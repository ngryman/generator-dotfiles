  "precommit": [
    "test",
    "check-coverage"
  ],
  "keywords": [
    "dotfiles",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "yeoman-generator": "^1.0.0",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "codecov.io": "^0.1.6",
    "eslint": "^3.0.0",
    "eslint-config-ngryman": "^1.1.0",
    "nyc": "^10.0.0",
    "pre-commit": "^1.1.3",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.0.0"
  }
}
